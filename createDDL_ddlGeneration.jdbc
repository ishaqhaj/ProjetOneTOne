CREATE TABLE students (ID BIGINT AUTO_INCREMENT NOT NULL, AGE INTEGER NOT NULL, EMAIL VARCHAR(255) NOT NULL UNIQUE, NAME VARCHAR(255) NOT NULL, PRIMARY KEY (ID))
CREATE TABLE addresses (ID BIGINT AUTO_INCREMENT NOT NULL, CITY VARCHAR(255) NOT NULL, COUNTRY VARCHAR(255) NOT NULL, STREET VARCHAR(255) NOT NULL, ZIPCODE VARCHAR(255) NOT NULL, student_id BIGINT, PRIMARY KEY (ID))
ALTER TABLE addresses ADD CONSTRAINT FK_addresses_student_id FOREIGN KEY (student_id) REFERENCES students (ID)
